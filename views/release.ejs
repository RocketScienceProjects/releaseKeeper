  <% include partials/header %>
  <div class="container">
    <header class="jumbotron">
      <div>
          <p>Salesforce releases deployment information for Global SalesForce Team - CBRE</p>
          <p>
            <a class="btn btn-primary btn-sm" href="/release/new">Add new release</a>
          </p>
      </div>
    </header>

     <div class="column">
        <% releases.forEach(function(release){ %>
          <div class="col-md-3 col-sm-6">
            <p background-color: yellow>
              <li>ORG_NAME: <%= release.org %></li>
               <li>PACKAGE: <%= release.package %></li>
               <li>DEPLOYED_ON: <%= release.date %></li>
            </p>
          </div>
        <% }) %>
     </div>

  </div>

  <% include partials/footer %>
